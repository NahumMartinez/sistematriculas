<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <resource-ref>
        <description>context</description>
        <res-ref-name>jdbc/SGCHON</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    <resource-ref>
        <description>context</description>
        <res-ref-name>jdbc/FORM</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>    
   <session-config>
        <session-timeout>
            100
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>open/index.jsp</welcome-file>
        <welcome-file>open/index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>MuestraRecibo</servlet-name>
        <servlet-class>com.soluzionasf.iopen.Util.MuestraRecibo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MuestraRecibo</servlet-name>
        <url-pattern>/MuestraRecibo</url-pattern>
    </servlet-mapping>
    <login-config>
        <auth-method>BASIC</auth-method>
    </login-config>
</web-app>
